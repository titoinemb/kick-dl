{
  "name": "kick-dl",
  "version": "1.0.0",
  "description": "The best CLI program for downloading Kick VODs",
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "jest ./src/tests/__test__.spec.ts",
    "build": "tsc && find ./dist -type f -name '*.js' -exec sh -c 'terser \"$1\" --compress --mangle --output \"${1%.js}.js\"' _ {} \\;",
    "link": "npm link",
    "unlink": "npm unlink -g kick-dl"
  },
  "bin": {
    "kick-dl": "./dist/index.js"
  },
  "keywords": [
    "kick",
    "vods",
    "kickvods",
    "kickvod",
    "streaming",
    "kickstreaming",
    "kickstream",
    "video",
    "videos",
    "kickvideos"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/titoinemb/kick-dl.git"
  },
  "author": "Ton Nom",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "jest": "^30.2.0",
    "terser": "^5.44.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "playwright": "^1.55.0"
  }
}
