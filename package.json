{
  "name": "@titoinemb/kick-dlp",
  "version": "1.0.1",
  "description": "The best CLI program for downloading Kick VODS",
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "jest ./src/tests/__test__.spec.ts",
    "build": "tsc && find ./dist -type f -name '*.js' -exec sh -c 'terser \"$1\" --compress --mangle --output \"${1%.js}.js\"' _ {} \\;",
    "link": "npm link",
    "unlink": "npm unlink -g kick-dlp"
  },
  "bin": {
    "kick-dlp": "./dist/index.js"
  },
  "keywords": [
    "kick",
    "vods",
    "kickvods",
    "kickvod",
    "streaming",
    "kickstreaming",
    "kickstream",
    "video",
    "videos",
    "kickvideos"
  ],
  "author": "titoinemb",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "jest": "^30.2.0",
    "terser": "^5.44.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "playwright": "^1.55.0"
  }
}
